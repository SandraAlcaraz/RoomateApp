<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="108dp"
        android:layout_marginRight="108dp"
        android:layout_marginBottom="28dp"
        android:clickable="true"
        android:onClick="editAnnouncement"
        app:backgroundTint="@color/btn_logut_bg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@android:drawable/ic_menu_edit" />

    <ImageButton
        android:id="@+id/profileButton2"
        android:layout_width="40dp"
        android:layout_height="49dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:background="@color/white"
        android:onClick="userProfileButtonA"
        android:scaleType="centerInside"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@mipmap/profile"
        tools:layout_editor_absoluteY="0dp" />

    <ScrollView
        android:id="@+id/scrollView60"
        android:layout_width="418dp"
        android:layout_height="576dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home5"
        app:layout_constraintVertical_bias="0.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ListView
                android:id="@+id/announcementsListView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:divider="@null"
                android:dividerHeight="0dp" />
        </LinearLayout>
    </ScrollView>

    <ImageButton
        android:id="@+id/home5"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:background="@color/white"
        android:onClick="homeButtonA"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/home"
        tools:layout_editor_absoluteY="0dp" />

    <ImageButton
        android:id="@+id/toDo5"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="12dp"

        android:layout_marginLeft="12dp"
        android:background="@color/white"
        android:onClick="toDoButtonA"
        app:layout_constraintStart_toEndOf="@+id/home5"
        app:srcCompat="@mipmap/check_circle"
        tools:layout_editor_absoluteY="0dp" />

    <ImageButton
        android:id="@+id/meetings5"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="12dp"

        android:layout_marginLeft="12dp"
        android:background="@color/white"
        android:onClick="meetingsButtonA"
        app:layout_constraintStart_toEndOf="@+id/toDo5"
        app:srcCompat="@mipmap/people"
        tools:layout_editor_absoluteY="0dp" />

    <ImageButton
        android:id="@+id/announcement5"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="16dp"

        android:layout_marginLeft="16dp"
        android:background="@color/white"
        android:onClick="announcementButtonA"
        android:tint="@color/colorPrimary"
        app:layout_constraintStart_toEndOf="@+id/meetings5"
        app:srcCompat="@mipmap/announcement"
        tools:layout_editor_absoluteY="0dp" />

    <ImageButton
        android:id="@+id/shopping5"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="16dp"

        android:layout_marginLeft="16dp"
        android:background="@color/white"
        android:onClick="shoppingButtonA"
        app:layout_constraintStart_toEndOf="@+id/announcement5"
        app:srcCompat="@mipmap/shopping_cart"
        tools:layout_editor_absoluteY="0dp" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:layout_marginBottom="31dp"
        android:clickable="true"
        android:onClick="createAnnouncements"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@android:drawable/ic_input_add" />

    <RelativeLayout
        android:id="@+id/relative"
        android:layout_width="match_parent"
        android:layout_height="263dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="50dp">

    </RelativeLayout>

</android.support.constraint.ConstraintLayout>