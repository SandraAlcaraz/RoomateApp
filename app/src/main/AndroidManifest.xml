<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sandra.roomate_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".GroupInformationActivity"></activity>
        <activity android:name=".ProfileActivity"></activity>
        <activity android:name=".CreateGroupActivity" />
        <activity android:name=".JoinGroupActivity" />
        <activity android:name=".CodeGeneratorActivity" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.HOME" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".RoomateListActivity" />
        <activity android:name=".ToDoActivity" />
        <activity android:name=".MeetingsActivity" />
        <activity android:name=".AnnouncementActivity" />
        <activity android:name=".ShoppingActivity" />
    </application>

</manifest>